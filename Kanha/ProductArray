import.java.io*;

public class productArray{

public static void main(String[] args){
int[] arr = {10,20,30,40};
int product = 1;
for(int i=0;i<arr.length;i++){
product*=arr[i];
}

final int temp = product;
for(int i=0;i<arr.length;i++){
arr[i] = temp/arr[i];
}

for(int i=0;i<arr.length;i++){
System.out.println(arr[i]+ " ");
}
}
}
