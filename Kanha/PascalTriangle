import java.io.*; 
  
class Pascal { 
    public static void main (String[] args) { 
        int n = 5; 
        Pascal(n); 
    } 
  
public static void Pascal(int n) 
{ 
int[][]arr = new int[n][n];  
for (int l = 0; l < n; l++) 
{ 
    for (int i = 0; i <= line; i++) 
    { 
    if (l == i || i == 0) 
        arr[l][i] = 1; 
    else 
        arr[l][i] = arr[l-1][i-1] + arr[l-1][i]; 
    System.out.print(arr[l][i]); 
    } 
    System.out.println(""); 
} 
} 
} 
